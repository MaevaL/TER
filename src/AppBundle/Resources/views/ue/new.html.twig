{% extends 'AppBundle::panel_layout.html.twig' %}

{% block panel_body %}
    <div class="showback">
        <div class="row">
            <div class="col-lg-12">
                <h3><i class="fa fa-angle-right"></i> Ajouter une UE</h3>
            </div>
        </div>

        <br>
        <div class="row">
            <div class="col-lg-12">
                <a class="btn btn-theme-amu" href="{{ path('ue_index') }}"><i class="fa fa-caret-left" aria-hidden="true"></i> Retour à la liste des UEs</a>
            </div>
        </div>
        <br>

        <div class="row">
            <div class="col-lg-12">
                {{ form_start(form) }}
                {{ form_widget(form) }}
                <button class="btn btn-success" type="submit"><i class="fa fa-check" aria-hidden="true"></i> Créer</button>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}
